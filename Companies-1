    print("yes")
else :
    print("no")

Ans :
N = int(input())
arr = [input() for _ in range(N)]

def a(str):
    vowels = "aAeEiIoOuU"
    for char in str :
        if char in vowels:
            return True
    return False

def b(arr) :
    for str in arr:
        if not a(str):
            return False
    return True

if b(arr):
    print("yes")
else:
    print("no")
